{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "quotes": [2, "double"],
        "import/extensions": ["error", "always", {"ignorePackages": true} ]
    }
}
